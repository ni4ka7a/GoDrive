@model GoDrive.Web.ViewModels.Organizations.OrganizationDetailsViewModel

<div class="row">
    <div class="col m6">
        <img class="materialboxed" width="400" src="http://www.volan-bg.com/static/images/cat_B1_01.jpg">
    </div>

    <div class="col m6">
        <h2>@Model.Name</h2>
        <p>@Model.AboutInfo</p>
        @Html.ActionLink("Join Now", "Register", "Account", routeValues: new { area = string.Empty }, htmlAttributes: new { @class = "aves-effect waves-light btn deep-purple darken-4" })
    </div>
</div>

<hr />

<div class="row white-text">
    <div class="col m6">
        <strong>Over than @Model.UsersCount Students</strong>
    </div>

    <div class="col m6">
        <strong>In The WebSite since @Model.CreatedOn</strong>
    </div>
</div>

<div class="row blue darken-1">
    <h4 class="center">Our Cars</h4>

    <div class="carousel">
        <a class="carousel-item" href="#1"><img src="http://car-pictures.cars.com/images/?IMG=cac10foc051c01401.png&WIDTH=624&AUTOTRIM=1&SPECIAL=&ACT=F"></a>
        <a class="carousel-item" href="#2"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTR4e1Bptu1sPdq_eYliY7JkeroHjhXv3fPHf3l33UZFijQ3nN7tw"></a>
        <a class="carousel-item" href="#3"><img src="http://img.autobytel.com/car-reviews/autobytel/11694-good-looking-sports-cars/2016-Ford-Mustang-GT-burnout-red-tire-smoke.jpg"></a>
    </div>
</div>

@section Scripts {
    <script>
        $(document).ready(function () {
            $('.materialboxed').materialbox();
            $('.carousel').carousel();
        });
    </script>
}