@model GoDrive.Web.ViewModels.Organizations.OrganizationViewModel

<div class="col s4">
    <div class="card">
        <div class="card-image waves-effect waves-block waves-light">
            <img class="activator" src="@Model.OrganizationImageUrl">
        </div>
        <div class="card-content">
            <span class="card-title activator grey-text text-darken-4">@Model.Name<i class="material-icons right">more_vert</i></span>
            <p><a class="waves-effect waves-light btn" href="/Organizations/Details/@Model.Id">Details</a></p>
        </div>
        <div class="card-reveal">
            <span class="card-title grey-text text-darken-4">@Model.Name<i class="material-icons right">close</i></span>
            <p>@Model.AboutInfo</p>
        </div>
    </div>
</div>
