@model IEnumerable<GoDrive.Web.Areas.MyOrganization.ViewModels.UserRequestViewModel>

<table class="centered">
    <thead>
        <tr class="white-text">
            <th data-field="price">Username</th>
            <th data-field="id">First Name</th>
            <th data-field="name">Last Name</th>
            <th data-field="price">Age</th>
            <th data-field="price">Additional Information</th>
            <th data-field="price">Created On</th>
            <th data-field="price">IsProceed</th>
            <th></th>
            <th></th>
        </tr>
    </thead>

    <tbody>

        @foreach (var request in Model)
        {
            <tr>
                <td>@request.Username</td>
                <td>@request.FirstName</td>
                <td>@request.LastName</td>
                <td>@request.Age</td>
                <td>@request.AdditionalInformation</td>
                <td>@request.CreatedOn</td>
                <td>@request.IsProceed</td>
                <td>
                    @Html.ActionLink("Add", "AddToOrganization", "ManageRequests", routeValues: new { userId = request.UserId, requestId = request.Id, area = "MyOrganization" }, htmlAttributes: new { @class = "aves-effect waves-light btn deep-purple darken-4" })
                </td>
                <td>
                    @Html.ActionLink("Decline", "RejectToOrganization", "ManageRequests", routeValues: new { userId = request.UserId, id = request.Id, area = "MyOrganization" }, htmlAttributes: new { @class = "aves-effect waves-light btn deep-purple darken-4" })
                </td>
            </tr>
        }

    </tbody>
</table>
