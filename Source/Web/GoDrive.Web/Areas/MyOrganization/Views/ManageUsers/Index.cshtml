@model IEnumerable<GoDrive.Web.Areas.MyOrganization.ViewModels.ManageUserViewModel>

<table class="centered">
    <thead>
        <tr class="white-text">
            <th data-field="id">Username</th>
            <th data-field="id">First Name</th>
            <th data-field="name">Last Name</th>
            <th data-field="price">Age</th>
            <th></th>
        </tr>
    </thead>

    <tbody>

        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Age</td>
                <td>
                    @Html.ActionLink("Schedular", "Register", "Account", routeValues: new { area = string.Empty }, htmlAttributes: new { @class = "aves-effect waves-light btn deep-purple darken-4" })
                </td>
            </tr>
        }

    </tbody>
</table>
